{
  "name": "nasa-system7-server",
  "version": "1.0.0",
  "description": "Backend server for the NASA System 7 Portal - NASA and JPL API integration with secure proxy and PostgreSQL database",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "db:init": "node -r dotenv/config -e \"require('./db.js').initDb()\"",
    "db:seed": "node -e \"require('./db.js').seedDb()\"",
    "db:migrate": "node -e \"require('./db.js').migrateDb()\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "performance:test": "node scripts/performanceTest.js",
    "performance:load": "node scripts/performanceTest.js load",
    "monitor": "node scripts/performanceMonitor.js",
    "cache:test": "node scripts/cacheTest.js",
    "cache:stats": "node scripts/cacheManager.js --stats",
    "cache:monitor": "node scripts/cacheMonitor.js",
    "cache:clear": "node scripts/cacheManager.js --clear-all",
    "cache:list": "node scripts/cacheManager.js --list",
    "websocket:test": "node scripts/websocketTest.js",
    "websocket:load": "node scripts/websocketLoadTest.js",
    "auth:test": "node scripts/authTest.js",
    "session:validate": "node scripts/sessionValidator.js",
    "nasa:stream:test": "node scripts/nasaStreamTest.js",
    "phase3:verify": "node scripts/phase3Verification.js",
    "phase3:verify:config": "node scripts/phase3Verification.js --config",
    "phase3:verify:quick": "node scripts/phase3Verification.js --quick",
    "test:ci": "jest --coverage --watchAll=false --ci",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --runInBand --testPathPattern=.*integration.test.js",
    "test:api": "jest --runInBand --testPathPattern=.*api.test.js"
  },
  "keywords": [
    "nasa",
    "api",
    "nodejs",
    "express",
    "postgresql",
    "space",
    "astronomy",
    "jpl"
  ],
  "author": "NASA System 7 Portal Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "pg-pool": "^3.6.1",
    "prom-client": "^14.2.0",
    "redis": "^4.6.10",
    "socket.io": "^4.8.1",
    "speakeasy": "^2.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.54.0",
    "eslint-config-node": "^4.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "socket.io-client": "^4.8.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  }
}
